// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "lib/**/*.js": { "when": "$(basename).ts" },
        "lib/**/*.js.map": true,
        "lib/**/*.d.ts": true,
        "example/**/*.js": { "when": "$(basename).ts" },
        "example/**/*.js.map": true,
        "example/**/*.d.ts": true
    }
}